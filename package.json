{
  "name": "@gliff-ai/dominate",
  "version": "1.7.0",
  "description": "gliff.ai DOMINATE",
  "repository": "git://github.com/gliff-ai/dominate.git",
  "main": "dist/main.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "NODE_OPTIONS='--max-old-space-size=8192' npx vite build --mode development",
    "build:staging": "NODE_OPTIONS='--max-old-space-size=8192' npx vite build --mode staging && cp staticwebapp.config.json dist/",
    "build:production": "NODE_OPTIONS='--max-old-space-size=8192' npx vite build --mode production && cp staticwebapp.config.json dist/",
    "checkDeps": "npm outdated `cat package.json | jq -r '.dependencies, .devDependencies | to_entries[] | select(.key|startswith(\"@gliff\")).key' | paste -sd ' ' -` ",
    "format": "npx --no-install prettier --write src",
    "prettier": "npx --no-install prettier -c src",
    "lint": "tsc --noEmit && npm run lint:ts && npm run prettier",
    "lint:ts": "npx eslint src",
    "serve": "npx vite",
    "test": "jest --coverage --passWithNoTests"
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@gliff-ai/eslint-config": "^0.2.0",
    "@types/libsodium-wrappers": "^0.7.9",
    "@types/logrocket-react": "^3.0.0",
    "@types/react": "^17.0.31",
    "@types/react-dom": "^17.0.2",
    "@types/resize-observer-browser": "^0.1.5",
    "eslint": "^7.25.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "prettier": "=2.3.0",
    "ts-jest": "^28.0.2",
    "typescript": "^4.2.5",
    "vite": "^2.5.3",
    "vite-aliases": "^0.8.6",
    "vite-plugin-checker": "^0.3.4",
    "vite-react-jsx": "^1.1.2"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@gliff-ai/annotate": "^4.4.5",
    "@gliff-ai/audit": "^2.1.2",
    "@gliff-ai/curate": "^6.0.0",
    "@gliff-ai/manage": "^5.0.5",
    "@gliff-ai/style": "^13.0.1",
    "@gliff-ai/upload": "^1.2.5",
    "@material-ui/types": "^5.1.0",
    "@mui/icons-material": "^5.3.0",
    "@mui/material": "^5.3.0",
    "@mui/styles": "^5.3.0",
    "@sentry/integrations": "^6.6.0",
    "@sentry/react": "^6.6.0",
    "@sentry/tracing": "^6.6.0",
    "@stripe/stripe-js": "^1.14.0",
    "ajv": "^8.6.2",
    "axios": "^0.24.0",
    "csstype": "^3.0.10",
    "etebase": "^0.43.1",
    "file-saver": "^2.0.5",
    "history": "==5.0.1",
    "jszip": "^3.7.0",
    "libsodium-wrappers": "^0.7.6",
    "logrocket": "^2.0.0",
    "logrocket-react": "^4.0.1",
    "path-browserify": "^1.0.1",
    "react": "^17.0.0",
    "react-cookie-consent": "^6.2.4",
    "react-dom": "^17.0.1",
    "react-inlinesvg": "^2.3.0",
    "react-router-dom": "^6.2.1",
    "react-use-intercom": "^1.5.1"
  }
}
