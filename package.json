{
  "name": "@gliff-ai/dominate",
  "version": "1.7.0",
  "description": "gliff.ai DOMINATE",
  "repository": "git://github.com/gliff-ai/dominate.git",
  "main": "dist/main.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "NODE_OPTIONS='--max-old-space-size=8192' npx vite build --mode development",
    "build:staging": "NODE_OPTIONS='--max-old-space-size=8192' npx vite build --mode staging && cp staticwebapp.config.json dist/",
    "build:production": "NODE_OPTIONS='--max-old-space-size=8192' npx vite build --mode production && cp staticwebapp.config.json dist/",
    "checkDeps": "npm outdated `cat package.json | jq -r '.dependencies, .devDependencies | to_entries[] | select(.key|startswith(\"@gliff\")).key' | paste -sd ' ' -` ",
    "format": "npx --no-install prettier --write src",
    "prettier": "npx --no-install prettier -c src",
    "lint": "tsc --noEmit && npm run lint:ts && npm run prettier",
    "lint:ts": "npx eslint src",
    "serve": "npx vite",
    "test": "jest --coverage --passWithNoTests"
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@gliff-ai/eslint-config": "^0.2.4",
    "@types/libsodium-wrappers": "^0.7.9",
    "@types/logrocket-react": "^3.0.0",
    "@types/react": "^17.0.48",
    "@types/react-dom": "^17.0.17",
    "@types/resize-observer-browser": "^0.1.7",
    "eslint": "^7.32.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "prettier": "=2.3.0",
    "ts-jest": "^28.0.8",
    "typescript": "^4.7.4",
    "vite": "^2.9.15",
    "vite-aliases": "^0.8.7",
    "vite-plugin-checker": "^0.3.4",
    "vite-react-jsx": "^1.1.2"
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@gliff-ai/annotate": "^6.1.0",
    "@gliff-ai/audit": "^4.0.1",
    "@gliff-ai/curate": "^7.7.2",
    "@gliff-ai/etebase": "^0.44.0",
    "@gliff-ai/manage": "^9.0.3",
    "@gliff-ai/style": "^16.3.1",
    "@gliff-ai/upload": "^1.3.0",
    "@material-ui/types": "^5.1.0",
    "@mui/icons-material": "^5.10.2",
    "@mui/material": "^5.10.2",
    "@mui/styles": "^5.10.2",
    "@mui/x-data-grid": "^5.15.3",
    "@sentry/integrations": "^6.19.7",
    "@sentry/react": "^6.19.7",
    "@sentry/tracing": "^7.37.1",
    "@stripe/stripe-js": "^1.35.0",
    "ajv": "^8.11.0",
    "axios": "^0.24.0",
    "csstype": "^3.1.0",
    "file-saver": "^2.0.5",
    "history": "5.0.1",
    "jszip": "^3.10.1",
    "libsodium-wrappers": "^0.7.10",
    "logrocket": "^2.2.1",
    "logrocket-react": "^4.0.1",
    "npm": "^8.18.0",
    "path-browserify": "^1.0.1",
    "react": "^17.0.0",
    "react-cookie-consent": "^6.4.1",
    "react-dom": "^17.0.2",
    "react-inlinesvg": "^2.3.0",
    "react-router-dom": "^6.3.0",
    "react-use-intercom": "^1.5.2"
  }
}
